{
  "build": {
    "core": "esp32",
    "extra_flags": "-DARDUINO_ESP32_PICO",
    "f_cpu": "240000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",
    "ldscript": "esp32_out.ld",
    "mcu": "esp32",
    "variant": "pico32"
  },
  "connectivity": [
    "wifi",
    "bluetooth",
    "ethernet",
    "can"
  ],
  "frameworks":  [
    "arduino",
    "espidf"
  ],
  "name": "ESP32 Pico Kit",
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 294912,
    "maximum_size": 1310720,
    "require_upload_port": true,
    "speed": 115200,
    "protocol": "esptool",
    "protocols": [
      "esptool"
    ]
  },
  "url": "http://esp-idf.readthedocs.io/en/latest/get-started/get-started-pico-kit.html",
  "vendor": "Espressif"
}
