    "arduino",
    "espidf"
  ],
  "name": "Ai-Thinker NodeMCU-32S2 (ESP-12K)",
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 327680,
    "maximum_size": 4194304,
    "require_upload_port": true,
    "speed": 460800
